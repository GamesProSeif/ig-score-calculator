{
  "name": "ig-score-calculator",
  "version": "1.0.2",
  "description": "A small website that calculates ig scores",
  "author": "GamesProSeif",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
